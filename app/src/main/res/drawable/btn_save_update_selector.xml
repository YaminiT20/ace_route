<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
	     <shape android:shape="rectangle"  >
	         <corners android:radius="3dip" />
	         <stroke android:width="1dip" android:color="#E67E22" />
	         <gradient android:angle="-90" android:startColor="#D35400" android:endColor="#E67E22" />            
	     </shape>
	 </item>
	
	<item android:state_focused="true">
	     <shape android:shape="rectangle"  >
	         <corners android:radius="3dip" />
	         <stroke android:width="1dip" android:color="#5e7974" />
	         <solid android:color="#E67E22"/>       
	     </shape>
	 </item>  
	
	<item>
	    <shape android:shape="rectangle"  >
	         <corners android:radius="3dip" />
	         <stroke android:width="1dip" android:color="#E67E22" />
	         <gradient android:angle="-90" android:startColor="#E67E22" android:endColor="#D35400" />            
	     </shape>
	 </item>

</selector>
